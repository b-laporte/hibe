{
  "name": "hibe",
  "version": "0.0.5",
  "description": "Lightweight data framework implementing eventual immutability paradigm",
  "keywords": [
    "datastore",
    "data",
    "store",
    "flux",
    "immutability",
    "immutableJS",
    "watch"
  ],
  "files": [
    "dist/hibe*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-laporte/hibe.git"
  },
  "homepage": "https://github.com/b-laporte/hibe.git#readme",
  "main": "dist/hibe.js",
  "types": "dist/hibe.d.ts",
  "author": "Bertrand Laporte",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "yarn run clean && tsc",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run test",
    "postversion": "git push && git push --tags",
    "test": "yarn run build && yarn run mocha -r ts-node/register src/**/*.spec.ts --reporter dot"
  },
  "license": "Apache-2.0",
  "bugs": {},
  "devDependencies": {
    "typescript": ">=2.9.2",
    "mocha": "^5.2.0",
    "@types/node": "^10.3.5",
    "@types/mocha": "^5.2.3",
    "ts-node": "^7.0.1"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "engines": {
    "node": ">=10.3.5"
  }
}