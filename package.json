    "rollup-plugin-typescript2": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-minify-es": "^1.1.1",
    "rollup-plugin-gzip": "^1.3.0",
    "uglify-es": "^3.3.9",
    "typescript": ">=2.9.2",
    "mocha": "^5.2.0",
    "@types/node": "^10.3.5",
    "@types/mocha": "^5.2.3"
